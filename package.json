{
  "name": "jellies",
  "version": "1.6.6",
  "private": false,
  "description": "A UI toolkit for Vue 2.x based on elementUI.",
  "author": "Haoran Yu",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "npm run build-theme; npm run build-bundle",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name index --dest package ./src/index.js",
    "build-theme": "node_modules/gulp/bin/gulp.js build"
  },
  "main": "./src/index.js",
  "module": "./src/index.js",
  "files": [
    "src/*",
    "package",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "copy-to-clipboard": "^3.3.1",
    "core-js": "^3.7.0",
    "element-ui": "2.13.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "sass": "^1.29.0",
    "splitpanes": "^2.3.6",
    "vue-codemirror": "^4.0.6",
    "vue-katex-auto-render": "^0.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.1.0",
    "node-sass": "^4.14.1",
    "node-sass-package-importer": "^5.3.2",
    "sass-loader": "^9.0.3",
    "vue": "^2.6.12",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "quote-props": [
        2,
        "as-needed"
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "license": "MIT",
  "repository": "https://github.com/haoranyu/jellies",
  "style": "package/index.css"
}
